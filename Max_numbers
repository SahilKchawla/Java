public class max
{
    public static void main(String args[]){
        int[]marks={125,10,200,30};
        int highest_marks=maximum(marks);
        System.out.println("the highest score is"+highest_marks);
    }
    public static int maximum(int[] numbers){
    int max=numbers[0];
    for(int n:numbers){
        if(n>max){
            max=n;
        }
    }
    return max;
}
}
